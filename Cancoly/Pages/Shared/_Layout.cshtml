<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Title -->
    <title>@ViewData["Title"] - Cancoly</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="landing/assets/img/brain.png">
    <link href="css/style.css" rel="stylesheet" type="text/css" />
    <link href="https://fonts.googleapis.com" rel="preconnect" />
    <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin="anonymous" />
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.26/webfont.js" type="text/javascript"></script>
    <script type="text/javascript">WebFont.load({  google: {    families: ["Inter:regular,500,600,700"]  }});</script>
    <script type="text/javascript">!function(o,c){var n=c.documentElement,t=" w-mod-";n.className+=t+"js",("ontouchstart"in o||o.DocumentTouch&&c instanceof DocumentTouch)&&(n.className+=t+"touch")}(window,document);</script>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/sweetalert2@11/dist/sweetalert2.min.css">
</head>

<body>
    <div class="page-wrapper">
        <div class="global-styles w-embed">
        </div>
        <main class="nav_fixed">
            <div data-animation="default" data-collapse="medium" data-duration="600" data-easing="ease-out-quint" data-easing2="ease-in-quad" role="banner" class="nav_component w-nav">
                <div class="padding-global">
                    <div class="nav_container">
                        <a href="/" aria-current="page" class="nav_brand w-nav-brand w--current">
                            <img src="logos/logo.png" style="height:40px;width:150px;filter: brightness(0) invert(1);" alt="" class="nav_logo" />
                        </a>
                        <nav role="navigation" class="nav_menu w-nav-menu">
                            <a href="/#features" class="nav_menu_link cc-caption w-nav-link">Features</a>
                            <a href="/#works" class="nav_menu_link cc-caption w-nav-link">How it works</a><a href="/#pricing" class="nav_menu_link cc-caption w-nav-link">Pricing</a>
                            <a href="/account-login" aria-current="page" class="cc-cta-link hide-desktop w-inline-block w--current">
                                <div class="cc-cta-text">Get Started</div>
                                <div class="cta-button-wrap">
                                    <div class="cta-button-wrap-icon w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                                    <div class="cta-button-wrap-icon is-absolute w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                                </div>
                            </a>
                        </nav>
                        <a href="/account-login" aria-current="page" class="cc-cta-link hide-tablet w-inline-block w--current">
                            <div class="cc-cta-text">Get Started</div>
                            <div class="cta-button-wrap">
                                <div class="cta-button-wrap-icon w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                                <div class="cta-button-wrap-icon is-absolute w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                            </div>
                        </a>
                        <div class="c-navbar_button w-nav-button">
                            <div class="hamburger_2_wrap">
                                <div class="hamburger_2_line"></div>
                                <div class="hamburger_embed w-embed">
                                    <style>
                                        .hamburger_2_wrap {
                                            --thickness: 0.1rem;
                                            --gap: 0.4rem;
                                            --rotate: 30;
                                            --width: 100%;
                                        }

                                        .w--open .hamburger_2_line:first-child {
                                            transform: translateY(calc(var(--thickness) * 0.5 + var(--gap) * 0.5)) rotate(calc(var(--rotate) * 1deg));
                                        }

                                        .w--open .hamburger_2_line:last-child {
                                            transform: translateY(calc(var(--thickness) * -0.5 + var(--gap) * -0.5)) rotate(calc(var(--rotate) * -1deg));
                                        }

                                        .hamburger_2_wrap:hover {
                                            --rotate: 45;
                                        }
                                    </style>
                                </div>
                                <div class="hamburger_2_line"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <main class="main-wrapper">
            @RenderBody()
            <footer class="cc-footer">
                <div class="footer_wrapper">
                    <div class="footer_container">
                        <div class="footer_top-container">
                            <img src="logos/logo.png" style="height:40px;width:150px;filter: brightness(0) invert(1);" alt="" class="footer_top-container-logo" />
                            <h2 class="footer_top-container-heading cc-heading-h4">Intelligent and automated diagnostic platform</h2>
                            <p class="footer_top-container-paragraph">This solution is a game changer for radiographers, easily diagnose patients by uploading MRI scans.</p>
                            <a data-w-id="5c97efe1-ae37-350c-8cfa-1ace1cecbb91" href="/account-login" aria-current="page" class="cc-cta-link w-inline-block w--current">
                                <div class="cc-cta-text">Get Started</div>
                                <div class="cta-button-wrap">
                                    <div class="cta-button-wrap-icon w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                                    <div class="cta-button-wrap-icon is-absolute w-embed"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/6760526c75d7a177d4a94b80_Vector%20(1).svg" /></div>
                                </div>
                            </a>
                        </div>
                        <div class="footer_bottom-wrapper">
                            <div class="footer_container-bottom">
                                <div class="footer_bottom-row-grid">
                                    <div class="footer_bottom-col flex-one">
                                        <a href="/" aria-current="page" class="footer_brand-icon-link w-inline-block w--current">
                                            <img src="logos/logo.png" style="height:40px;width:150px;filter: brightness(0) invert(1);" loading="lazy" alt="" class="footer_brand-icon" />
                                        </a>
                                        <p class="footer_bottom-text cc-body-small">Empower your workflow with intelligent AI solution that transform your approach and streamline diagnostic operations</p>
                                    </div>
                                    <div class="footer_bottom-col flex-two">
                                        <h2 class="footer_bottom-heading cc-subtitle">Products</h2>
                                        <ul role="list" class="footer_list">
                                            <li class="footer_list-item"><a href="/#features" class="footer_list-item-link">Features</a></li>
                                            <li class="footer_list-item"><a href="/#works" class="footer_list-item-link">How it works</a></li>
                                            <li class="footer_list-item"><a href="/#faqs" class="footer_list-item-link">FAQs</a></li>
                                            <li class="footer_list-item"><a href="/#pricing" class="footer_list-item-link">Pricing</a></li>
                                        </ul>
                                    </div>
                                    <div class="footer_bottom-col flex-two">
                                        <h2 class="footer_bottom-heading cc-subtitle">Pages</h2>
                                        <ul role="list" class="footer_list">
                                            <li class="footer_list-item"><a href="/account-login" class="footer_list-item-link">Login</a></li>
                                            <li class="footer_list-item"><a href="/account-register" class="footer_list-item-link">Register</a></li>
                                        </ul>
                                    </div>
                                    <div class="footer_bottom-col flex-two">
                                        <h2 class="footer_bottom-heading cc-subtitle">Contact</h2>
                                        <ul role="list" class="footer_list">
                                            <li class="footer_list-item"><a href="" class="footer_list-item-link">help@cancoly.com</a></li>
                                            <li class="footer_list-item"><a href="" class="footer_list-item-link">+234 8093 7238 42</a></li>
                                         </ul>
                                        <div class="footer_social-links-wrap"><a href="/" aria-current="page" class="footer_social-link w-inline-block w--current"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/67618e0d22b5f70dca77e232_Vector%20(1).png" loading="lazy" alt="Instagram Icon" class="footer_social-link-icon" /></a><a href="/" aria-current="page" class="footer_social-link w-inline-block w--current"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/67619b188d45b9b09a7b4453_Vector%20(7).svg" loading="lazy" alt="Twitter Icon" class="footer_social-link-icon" /></a><a href="/" aria-current="page" class="footer_social-link w-inline-block w--current"><img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/67618d9ea0da596f8f13bf9f_Vector%20(3).svg" loading="lazy" alt="Youtube Icon" class="footer_social-link-icon" /></a></div>
                                    </div>
                                </div>
                                <div class="footer_row-grid">
                                    <p class="footer_row-text">© Cancoly 2025. All Rights Reserved.</p>

                                </div>
                            </div>
                        </div>
                        <div class="footer_ellipsis-wrapper">
                            <img src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/67619b187f63dfd24900f467_image%2029.webp" loading="lazy" 
                            sizes="100vw" alt=""
                           srcset="https://cdn.prod.website-files.com/6760415a4c2b104632244403/67619b187f63dfd24900f467_image%2029-p-500.webp 500w,
                            https://cdn.prod.website-files.com/6760415a4c2b104632244403/67619b187f63dfd24900f467_image%2029.webp 518w" 
                            class="ellipsis_image-round" /></div>
                    </div>
                </div>
            </footer>
        </main>
    </div>


    @await RenderSectionAsync("Scripts", required: false)
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=6760415a4c2b104632244403" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    <script src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/js/oma-voxia.schunk.36b8fb49256177c8.js" type="text/javascript"></script>
    <script src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/js/oma-voxia.schunk.291140fb4eceeab0.js" type="text/javascript"></script>
    <script src="https://cdn.prod.website-files.com/6760415a4c2b104632244403/js/oma-voxia.c6f2e7e2.87cfe0fd1f69514c.js" type="text/javascript"></script>

    @if (TempData["AlertMessage"] != null)
    {
        <script>
            Swal.fire({
                title: '@TempData["AlertSubject"]!',
                text: '@TempData["AlertMessage"]',
                icon: '@TempData["AlertType"]',
                confirmButtonText: 'Got it!'
            });
        </script>
    }
</body>
</html>